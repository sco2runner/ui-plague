for _, obj in ipairs(ReplicatedStorage:GetChildren()) do
    if obj:IsA("Folder") and string.find(obj.Name, "AlpineAnticheat") then
        print("Destroying folder:", obj.Name)
        obj:Destroy()
    end
end

        getgenv().ED_AntiKick = {Enabled = true, SendNotifications = false, CheckCaller = false}
    end
    -- ===== BYPASS END =====
